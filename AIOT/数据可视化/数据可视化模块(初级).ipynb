   "source": [
    "x = np.linspace(0, 2*np.pi,100)\n",
    "y = np.sin(x)\n",
    "plt.plot(x, y)\n",
    "# 设置坐标的范围\n",
    "plt.xlim(-1, 10)\n",
    "plt.ylim(-1.5, 1.5) \n",
    "# 网格线 alpha:透明度\n",
    "plt.grid(color='green', alpha=0.2, linestyle='--')"
